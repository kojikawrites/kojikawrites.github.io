// //      "src/scripts/client/**/*.ts"
// //    ],
    "baseUrl": ".",
    "paths": {
      "@client-scripts/*": ["/src/scripts/client/*"],
    },
//    "plugins": [
//      {
//        "name": "@astrojs/ts-plugin"
//      },
//    ]
  }
}
